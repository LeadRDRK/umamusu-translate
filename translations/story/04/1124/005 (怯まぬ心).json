            "enText": "",
            "nextBlock": 20,
            "origClipLength": 107,
            "voiceIdx": -1,
            "choices": [
                {
                    "jpText": "お疲れ様、少し休憩しないか？",
                    "enText": "",
                    "nextBlock": 20
