            "pathId": -2218280567492129512,
            "blockIdx": 9
        },
        {
            "jpName": "バブルガムフェロー",
            "enName": "",
            "jpText": "……じゃあ、私がやるよ！",
            "enText": "",
            "nextBlock": 11,
            "origClipLength": 97,
            "voiceIdx": 8,
            "choices": [
                {
                    "jpText": "えっ……！？そんなことできるのか！？",
                    "enText": "",
                    "nextBlock": 11
                },
                {
                    "jpText": "えっ……！？そんなことできるの！？",
                    "enText": "",
                    "nextBlock": 11
                }
